# Public checkout URL used by the sticky buy button and call-to-action links.
NEXT_PUBLIC_CHECKOUT_URL=

# Download URL where purchasers can obtain the ZIP if not delivered automatically by the payment provider.
NEXT_PUBLIC_DOWNLOAD_URL=

# Optional: link to a support policy or FAQ on another site.
NEXT_PUBLIC_SUPPORT_POLICY_URL=

# Webhook endpoint configured in Make for support requests. Do not prefix with NEXT_PUBLIC as the server function uses it directly.
MAKE_SUPPORT_WEBHOOK_URL=

# Webhook endpoint configured in Make for upsell (additional advisory) requests.
MAKE_UPSELL_WEBHOOK_URL=

# Optional support email to display on the success page.
NEXT_PUBLIC_SUPPORT_EMAIL=